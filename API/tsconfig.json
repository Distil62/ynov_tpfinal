{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es6",
    "lib": [
      "es2015",
      "es2017",
      "dom",
      "esnext",
      "es6"
    ],
    "sourceMap": true,
    "allowJs": false,
    "jsx": "preserve",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": false,
    "isolatedModules": true,
    "allowUmdGlobalAccess": true,
    "alwaysStrict": true,
    "typeRoots": ["node_modules/@types", "types"],
    "outDir": "./built"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "./server/**/*.ts",
    "./server/**/*.tsx",
    "server/auth/**/*.ts",
    "server.ts"
  ]
}
